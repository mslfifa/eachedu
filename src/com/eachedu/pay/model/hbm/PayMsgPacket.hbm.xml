<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.pay.model.PayMsgPacket" table="pay_msg_packet">
		<id name="packetId" column="packet_id">
			<generator class="native"/>
		</id>
		<property name="orderNo" column="order_no"/>
		<property name="creatTime" column="creat_time"/>
		<property name="msgType" column="msg_type"/>
		<property name="msgPacket" column="msg_packet"/>
	</class>
</hibernate-mapping>