<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.CoursewareInfo" table="courseware_info">
		<id name="ciId" column="ci_id">
			<generator class="native"/>
		</id>
		<property name="gciId" column="gci_id"/>
		<property name="courseTitle" column="course_title"/>
		<property name="introduce" column="introduce"/>
		<property name="picPath" column="pic_path"/>
		<property name="prise" column="prise"/>
		<property name="status" column="status"/>
	</class>
</hibernate-mapping>