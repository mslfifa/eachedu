<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.PlatFinance" table="plat_finance">
		<id name="pfId" column="pf_id">
			<generator class="native"/>
		</id>
		<property name="busType" column="bus_type"/>
		<property name="busId" column="bus_id"/>
		<property name="accountType" column="account_type"/>
		<property name="accountId" column="account_id"/>
		<property name="amount" column="amount"/>
		<property name="refunded" column="is_refunded"/>
		<property name="cashFlow" column="cash_flow"/>
		<property name="opTime" column="op_time"/>
	</class>
</hibernate-mapping>