<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.QuestionInfo" table="question_info">
		<id name="qiId" column="qi_id">
			<generator class="native"/>
		</id>
		<property name="gciId" column="gci_id"/>
		<property name="siId" column="si_id"/>
		<property name="communicateWay" column="communicate_way"/>
		<property name="questionDesc" column="question_desc"/>
		<property name="picId" column="pic_id"/>
		<property name="mobile" column="mobile"/>
		<property name="askTime" column="ask_time"/>
		<property name="status" column="status"/>
		<property name="remark" column="remark"/>
	</class>
</hibernate-mapping>