<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.StudentInfo" table="student_info">
		<id name="si_id" column="si_id">
			<generator class="native"/>
		</id>
		<property name="account" column="account"/>
		<property name="passWord" column="pass_word"/>
		<property name="status" column="status"/>
		<property name="nickname" column="nickname"/>
		<property name="name" column="name"/>
		<property name="sex" column="sex"/>
		<property name="grade" column="grade"/>
		<property name="mobile" column="mobile"/>
		<property name="email" column="email"/>
		<property name="mainPage" column="main_page"/>
		<property name="weibo" column="weibo"/>
		<property name="qq" column="qq"/>
		<property name="bankAccount" column="bank_account"/>
		<property name="alipay" column="alipay"/>
		<property name="createTime" column="create_time"/>
	</class>
</hibernate-mapping>