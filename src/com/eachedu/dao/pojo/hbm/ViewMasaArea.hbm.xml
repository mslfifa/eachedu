<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.ViewMasaArea" table="view_masa_area">
		<id name="areaId" column="area_id">
			<generator class="assigned"/>
		</id>
		<property name="areaName" unique="true" column="area_name"/>		
		<property name="areaDesc" column="area_desc"/>
		<property name="parentAreaId" column="parent_area_id"/>
		<property name="areaPath" column="area_path"/>
		<property name="areaPathCode" column="area_path_code"/>
		<property name="sortOrder" column="sort_order"/>
		<property name="flagGroup" column="flag_group"/>
		<property name="regulared" column="is_regular"/>
		<property name="superusered" column="if_superuser"/>
	</class>
</hibernate-mapping>