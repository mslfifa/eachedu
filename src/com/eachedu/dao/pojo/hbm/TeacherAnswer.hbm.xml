<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.TeacherAnswer" table="teacher_answer">
		<id name="taId" column="ta_id">
			<generator class="native"/>
		</id>
		<property name="tiId" column="ti_id"/>
		<property name="oiId" column="oi_id"/>
		<property name="answerContent" column="answer_content"/>
		<property name="picId" column="pic_id"/>
		<property name="status" column="status"/>
		<property name="connectType" column="connect_type"/>
		<property name="assignTime" column="assign_time"/>
		<property name="answerTime" column="answer_time"/>
	</class>
</hibernate-mapping>
