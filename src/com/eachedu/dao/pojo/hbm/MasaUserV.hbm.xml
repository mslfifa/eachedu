<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.MasaUserV" table="masa_user_v">
		<id name="userid" column="f_userid">
			<generator class="assigned"/>
		</id>
		<property name="username" unique="true" column="f_name"/>		
		<property name="email" column="f_email"/>
		<property name="mobile" column="f_mobile"/>
		<property name="password" column="f_password"/>
		<property name="flag" column="f_flag"/>
		<property name="registertime" column="f_registertime"/>
		<property name="validdate" column="f_validdate"/>
		<property name="hostid" column="f_hostid"/>
		<property name="regular" column="is_regular"/>
		<property name="superuser" column="if_superuser"/>
		<property name="areaid" column="f_areaid"/>
		<property name="groupid" column="f_groupid"/>
		<property name="ipSeq" column="ip_seq"/>
	</class>
</hibernate-mapping>