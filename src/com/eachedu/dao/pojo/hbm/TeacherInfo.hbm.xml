		<property name="weixin" column="weixin"/>
		<property name="qq" column="qq"/>
		<property name="weibo" column="weibo"/>
		<property name="mobile" column="mobile"/>
		<property name="email" column="email"/>
		<property name="tagId1" column="tag_id1"/>
		<property name="tagId2" column="tag_id2"/>
		<property name="tagId3" column="tag_id3"/>
		<property name="tagId4" column="tag_id4"/>
		<property name="tagId5" column="tag_id5"/>
		<property name="tagId6" column="tag_id6"/>
		<property name="tagTitle1" column="tag_title1"/>
		<property name="tagTitle2" column="tag_title2"/>
		<property name="tagTitle3" column="tag_title3"/>
		<property name="tagTitle4" column="tag_title4"/>
		<property name="tagTitle5" column="tag_title5"/>
		<property name="tagTitle6" column="tag_title6"/>
		<property name="createTime" column="create_time"/>
	</class>
</hibernate-mapping>