<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.ComplainSolve" table="complain_solve">
		<id name="csId" column="cs_id">
			<generator class="native"/>
		</id>
		<property name="orderId" column="order_id"/>
		<property name="complainType" column="complain_type"/>
		<property name="complainContext" column="complain_context"/>
		<property name="complanStatus" column="complan_status"/>
		<property name="complanTime" column="complan_time"/>
		<property name="solveTime" column="solve_time"/>
		<property name="solveContent" column="solve_content"/>
		<property name="score" column="score"/>
		<property name="evaluationContent" column="evaluation_content"/>
	</class>
</hibernate-mapping>