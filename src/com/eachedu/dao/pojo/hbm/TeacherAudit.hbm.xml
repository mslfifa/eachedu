<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.TeacherAudit" table="teacher_audit">
		<id name="taId" column="ta_id">
			<generator class="native"/>
		</id>
		<property name="passed" column="is_passed"/>
		<property name="rejectReason" column="reject_reason"/>
		<property name="auditTime" column="audit_time"/>
		<property name="auditorId" column="auditor_id"/>
	</class>
</hibernate-mapping>