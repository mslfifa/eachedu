<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.DimensionIndexInfo" table="dimension_index_info">
		<id name="diiId" column="dii_id">
			<generator class="native"/>
		</id>
		<property name="dscId" column="dsc_id"/>		
		<property name="fieldDbName" column="field_db_name"/>
		<property name="fieldBusName" column="field_bus_name"/>
		<property name="fieldType" column="field_type"/>
		<property name="createId" column="create_id"/>
		<property name="createTime" column="create_time"/>
	</class>
</hibernate-mapping>