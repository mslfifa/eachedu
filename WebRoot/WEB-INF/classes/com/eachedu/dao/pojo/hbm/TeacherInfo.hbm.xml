<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.TeacherInfo" table="teacher_info">
		<id name="tiId" column="ti_id">
			<generator class="native"/>
		</id>
		<property name="account" column="account"/>
		<property name="password" column="password"/>
		<property name="name" column="name"/>
		<property name="sex" column="sex"/>
		<property name="identificationCard" column="identification_card"/>
		<property name="school" column="school"/>
		<property name="authenticationType" column="authentication_type"/>
		<property name="headShortId" column="head_short_id"/>
		<property name="certificateId" column="certificate_id"/>
		<property name="weixin" column="weixin"/>
		<property name="qq" column="qq"/>
		<property name="weibo" column="weibo"/>
		<property name="mobile" column="mobile"/>
		<property name="email" column="email"/>
		<property name="tag1" column="tag_1"/>
		<property name="tag2" column="tag_2"/>
		<property name="tag3" column="tag_3"/>
		<property name="tag4" column="tag_4"/>
		<property name="tag5" column="tag_5"/>
		<property name="tag6" column="tag_6"/>
		<property name="createTime" column="create_time"/>
	</class>
</hibernate-mapping>