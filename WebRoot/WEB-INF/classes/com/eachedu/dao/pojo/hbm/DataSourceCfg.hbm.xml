<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.eachedu.dao.pojo.DataSourceCfg" table="data_source_cfg">
		<id name="dscId" column="dsc_id">
			<generator class="native"/>
		</id>
		<property name="tableDbName" column="table_db_name"/>
		<property name="tableBusName" column="table_bus_name"/>
		<property name="createId" column="create_id"/>
		<property name="createTime" column="create_time"/>
	</class>
</hibernate-mapping>